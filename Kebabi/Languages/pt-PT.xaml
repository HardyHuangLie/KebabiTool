<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                 xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <sys:String x:Key="CreatShortCut">Criar atalho na área de trabalho</sys:String>
    <sys:String x:Key="Start">Iniciar jogo</sys:String>
    <sys:String x:Key="Null">Nulo</sys:String>
    <sys:String x:Key="FailMsg1">Falha ao abrir o programa GenshinImpact, por favor selecione a opção de fechar proteção e reinicie o jogo</sys:String>
    <sys:String x:Key="FailMsg2">Falha na modificação, não foi possível escrever na memória</sys:String>
    <sys:String x:Key="Close">Fechar jogo</sys:String>
    <sys:String x:Key="FailMsg3">Falha ao criar atalho</sys:String>
    <sys:String x:Key="CloseProtect">Fechar proteção</sys:String>
    <sys:String x:Key="FailMsg4">Não encontrado, execute o injector.exe primeiro</sys:String>
    <sys:String x:Key="FailMsg5">Jogo não encontrado</sys:String>
    <sys:String x:Key="FailMsg6">O jogo está em execução, feche-o primeiro</sys:String>
</ResourceDictionary>